
public class Switch {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		char operatorMath ='-';
        double num1 = 20.0;
        double num2 = 24.5;
        double result;
        
		
		switch(operatorMath){
  	  case('+'):
  		  result = num1+num2;
  		  System.out.println(num1+num2);
  	break;
  	case('-'):
  		result = num1-num2;
		  System.out.println(num1%num2);
  	break;
  	case('/'):
  		result=num1/num2;
		  System.out.println(num1/num2);
  	break;
  	case('*'):
  		result=num1*num2;
		  System.out.println(num1*num2);
	  break;
  	case('%'):
  		result=num1%num2;
		  System.out.println(num1%num2);
	  break;

		}
  		  
  	  }

}
