
public class BooleanTasks {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		boolean isJavaFun=true;
				boolean isFishTasty=false;

				System.out.println(isJavaFun);
				System.out.println(isFishTasty);
				
				int x = 10;
				int y = 10;
				
				System.out.println(x<y);
				System.out.println(x==y);
					
	}

}
