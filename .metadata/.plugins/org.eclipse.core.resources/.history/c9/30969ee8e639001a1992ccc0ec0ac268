
public class ForLoops {

	/**
	 * @param args
	 */
	public static void main( String[] args) {
		
		System.out.println("********************");
		
		for(int i = 90; i<=100; i++){
			System.out.println("Number "+i);
	}
		System.out.println("******************");
		
		for(int i = 100; i>90; i--){
			System.out.println("Number "+i);
	}
}
}
