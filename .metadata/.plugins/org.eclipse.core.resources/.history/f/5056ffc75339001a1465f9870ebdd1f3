
public class OperatorsMath {


	public static void main(String[] args) {
	double num1 = 0.4d;
	double num2 = 0.3d;
	double result;
	char operators = '+';
	char operators1 = '-';
	char operators2 = '*';
	char operator3 = '/';
	result = num1 + num2;
	result = num1 - num2;
	result = num1 * num2;
	result = num1 / num2;
	
	System.out.println(num1 + " "+operators+" "+num2+" = "+result);
    System.out.println();
	System.out.println(num1 + " "+operators1+""+num2+" = "+result);
	System.out.println();
	System.out.println(num1 + " "+operators2+" "+num2+" = "+result);
	System.out.println();
	System.out.println(num1 + " "+operators+" "+num2+" = "+result);
	System.out.println();
	System.out.println(num1 + " "+operators+" "+num2+" = "+result);
	}

}
