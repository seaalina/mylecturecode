package lv.alina.player;

public class FootballPlayer {
String name;	
String position;
int shirtNumber;
boolean build;
long height;
int weight;

public String getName(){
	return name;
}
public String getPosition(){
	return position;
}
public int getShirtNumber(){
	return shirtNumber;
}
 public boolean getBuild(){
	return build;
}
public long getHeight(){
	return height;
}
public int getWeight(){
	return weight;
}
}