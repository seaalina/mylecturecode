
public class ForEach {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[]marks={38, 5, 22, 48, 57};
//For-EACH Loop (variable:arrays)
		for(int mark:marks){
			System.out.println(mark);
	}
		System.out.println("***************");
		for(int i=0; i<marks.length;i++){
			System.out.println(marks[i]);
	}
	}
}
