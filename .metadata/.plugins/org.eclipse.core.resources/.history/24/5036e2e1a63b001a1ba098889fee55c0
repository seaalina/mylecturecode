import java.util.Arrays;


public class ArraysRepeat {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		double[] varArrays;
//new
		varArrays=new double[3];
		varArrays[0]=0.5;
		varArrays[1]=1.0;
		varArrays[2]='a';
		char charVar=95;
		
		System.out.println(charVar);
		
		System.out.println(Arrays.toString(varArrays));
	}

}
