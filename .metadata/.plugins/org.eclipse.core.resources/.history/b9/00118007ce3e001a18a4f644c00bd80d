package lv.alina;

public class LearnMethodsWithTypeJava {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// method return type double
getArea();
System.out.println(getArea());
	}
	public static double getArea(){
		double num1=20.3;
		double num2=10.0;
		double result=num1*num2;
		return 10.0*20.3;
		
	}
	
}
