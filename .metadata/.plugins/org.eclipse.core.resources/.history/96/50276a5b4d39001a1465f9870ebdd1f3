
public class PrimType {

	
	public static void main(String[] args) {
		// Numeric data primitive type
		int integer = 2;
		long longInteger = 202040402980L;
		short shortInteger = 2233;
		byte byteInteger = 120;
		// Floating-Point type real numbers
		double doubleFloutNumber = 0.5;
		float floatNumber = 0.123456789f;
		// Characters
		char charSymbol = '$';
		char charSymbolAsNumber = 65;	
		char charSymbolAsNumber1 = 76;
		char charSymbolAsNumber2 = 105;
		char charSymbolAsNumber3 = 110;
		char charSymbolAsNumber4 = 97;
		System.out.println("Char: "+charSymbolAsNumber++charSymbolAsNumber1+charSymbolAsNumber2+charSymbolAsNumber3);
				

	}

}
