package lv.alina;

import java.util.ArrayList;

public class TaskTestArrayMethods {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
ArrayList<String> cars = new ArrayList<>();
		
		cars.add("KIA");
		cars.add("Volvo");
		cars.add("Mazda");
		cars.add("Reno");
		cars.add("Peugeut");
		cars.add("Opel");
		cars.add("Audi");
		System.out.println();
		for(String car: cars){
System.out.println(car);

		}
		System.out.println();
		cars.remove("Volvo");
		for (String car:cars){
			System.out.println(car);
		}
	}

}
